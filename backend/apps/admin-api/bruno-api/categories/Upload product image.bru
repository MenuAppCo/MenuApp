meta {
  name: Upload category image
  type: http
  seq: 8
}

post {
  url: {{API_URL}}/api/v1/uploads/categories/{{CATEGORY_ID}}
  body: multipartForm
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:multipart-form {
  image: @file(/home/rojbar/Downloads/sancocho.jpg) @contentType(image/jpeg)
}

body:file {
  file: @file()
}
